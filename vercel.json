{
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "build": {
      "env": {
        "PIP_EXTRA_INDEX_URL": "https://wheels.vercel.app/mysql-client/"
      }
    }
  }